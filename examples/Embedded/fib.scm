;*---------------------------------------------------------------------*/
;*    The module                                                       */
;*---------------------------------------------------------------------*/
(module fib)

;*---------------------------------------------------------------------*/
;*    fib ...                                                          */
;*---------------------------------------------------------------------*/
(define (fib x) (if (<fx x 2) 1 (+fx (fib (-fx x 1)) (fib (-fx x 2)))))

;*---------------------------------------------------------------------*/
;*    Runtime execution                                                */
;*---------------------------------------------------------------------*/
(print "fib( 20 ): " (fib 20))
