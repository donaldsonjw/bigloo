;*    Le module auxiliaire pour tester les alias                       */
;*=====================================================================*/

;*---------------------------------------------------------------------*/
;*    Le module                                                        */
;*---------------------------------------------------------------------*/
(module alias-aux
   (export (foo1 x)
	   (foo2 . x)
	   (foo3 x . y)))

;*---------------------------------------------------------------------*/
;*    foo1 ...                                                         */
;*---------------------------------------------------------------------*/
(define (foo1 x)
   x)

;*---------------------------------------------------------------------*/
;*    foo2 ...                                                         */
;*---------------------------------------------------------------------*/
(define (foo2 . x)
   x)

;*---------------------------------------------------------------------*/
;*    foo3 ...                                                         */
;*---------------------------------------------------------------------*/
(define (foo3 x . y)
   (cons x y))

